CREATE DATABASE FTM 
USE FTM


SET ANSI_NULLS ON
SET QUOTED_IDENTIFIER ON

CREATE TABLE Mon
(
MAMON INT NOT NULL PRIMARY KEY,
TEN NVARCHAR(50),
)


CREATE TABLE CauHoi
(
 NOIDUNG NVARCHAR(3000) NOT NULL,
 MAMON INT NOT NULL, 
 DA1 NVARCHAR(3000),
 DA2 NVARCHAR(3000),
 DA3 NVARCHAR(3000),
 DA4 NVARCHAR(3000),
 DA CHAR NOT NULL,
)

ALTER TABLE CauHoi 
ADD CONSTRAINT FK_CH_M_MAMON FOREIGN KEY (MAMON) REFERENCES Mon(MAMON)

